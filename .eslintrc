{
  "root": true,
  "rules": {
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-ex-assign": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "no-fallthrough": 2,
    "quotes": [2, "single", "avoid-escape"],
    "indent": [2, 2],
    "comma-spacing": 2,
    "semi": 2,
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-in-parens": [2, "never"],
    "space-before-blocks": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-before-blocks": [2, "always"],
    "new-parens": 2,
    "max-len": [2, 100, 2],
    "no-multiple-empty-lines": [2, {"max": 2}],
    "eol-last": 2,
    "no-trailing-spaces": 2,
    "strict": [2, "function"],
    "dot-notation": 0,
    "no-use-before-define": 0,
    "semi-spacing": 0,
    "no-undef": 2
  }
}
